      int a = 51;
			double c = 5.2342;
			char d = 'k';
			String e = "ankara";
			boolean cevap = true;

			System.out.println("A nin degeri " + a);
			System.out.println("C nin degeri " + c);
			System.out.println("D nin degeri " + d);
			System.out.println("E nin degeri " + e);
			System.out.println(cevap);
      
      
